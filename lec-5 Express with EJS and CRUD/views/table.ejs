<!DOCTYPE html>
<html lang="en">
<head>
    <title>stuent data</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
  </head>
</head>
<body>
    <center>
        <h1 class="my-4">Hey !! <%= name%>
        </h1>

        <div class="container col-6">
            <table class="table table-striped">
                <tr class="text-center">
                    <th>No</th>
                    <th>Name</th>
                    <th>Age</th>
                    <th>Email</th>
                    <th>Phone</th>
                    <th>Password</th>
                    <th colspan="2">Action</th>
                </tr>


                <% studdata.map((e,i)=> { %>
                    <tr class="text-center">
                        <td>
                            <%= i+1 %>
                        </td>
                        <td>
                            <%= e.name %>
                        </td>
                        <td>
                            <%= e.age %>
                        </td>
                        <td>
                            <%= e.email %>
                        </td>
                        <td>
                            <%= e.phone %>
                        </td>
                        <td>
                            <%= e.password %>
                        </td>
                        <td>
                            <button class="btn btn-success"><a style="color: white;" href="">Update</a></button>
                        </td>
                        <td>
                            <button class="btn btn-danger"><a style="color: white;" href="">Delete</a></button>
                        </td>
                    </tr>
                    <% }) %>
            </table>
        </div>

        <br>
        <a href="/insert">Add Students</a>
    </center>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
</body>
</html>